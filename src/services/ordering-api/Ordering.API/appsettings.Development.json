{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Ordering.API.Infrastructure.Outbox": "Warning",
      "Ordering.API.Infrastructure.Inbox": "Warning"
    }
  },
  "Outbox": {
    "BatchSize": 20,
    "Schedule": "0/15 * * * * *"
  },
  "Inbox": {
    "BatchSize": 20,
    "Schedule": "0/15 * * * * *"
  },
  "OpenApi": {
    "Title": "Ordering API",
    "RoutePattern": "/openapi/{documentName}"
  }
}
